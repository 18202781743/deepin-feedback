#! /bin/sh
set -e
install_feedback_desktop ()
{
	username=`users`
	if [ -e "/usr/share/applications/deepin-feedback.desktop" ]; then
		for i in $username ; do
			if [ -d "/home/$i/Desktop" ]; then
				cp /usr/share/applications/deepin-feedback.desktop /home/$i/Desktop
			fi
		done
	fi

}
case $1 in
    configure)
        install_feedback_desktop 
	;;
esac
exit 0

