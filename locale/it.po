#: ../gui/src/views/AdjunctPanel.qml:41
msgid "Failed to add attachment: Single attachment cann't be more than 5M."
msgstr "Caricamento allegato fallito: un singolo file non può superare i 5 Mb."

#: ../gui/src/views/AdjunctPanel.qml:127
msgid "Please select attachment"
msgstr "Per cortesia aggiungi un allegato"

#: ../gui/src/views/AdjunctPanel.qml:162 ../gui/src/views/AdjunctPanel.qml:183
#: ../gui/src/views/MainWindow.qml:225
msgid "Total attachments have reached limit. "
msgstr "Raggiunto il numero massimo di allegati."

#: ../gui/src/views/AdjunctPanel.qml:248
msgid "Please describe your problem in detail"
msgstr "Descrivi in dettaglio la problematica rilevata"

#: ../gui/src/views/AdjunctPanel.qml:249
msgid "Please upload related screenshots or files"
msgstr "Per cortesia carica eventuali screenshots o files"

#: ../gui/src/views/AdjunctPanel.qml:250 ../gui/src/views/AdjunctPanel.qml:273
msgid "Single attachment can not exceed 5M"
msgstr "Il singolo file non può superare i 5 Mb."

#: ../gui/src/views/AdjunctPanel.qml:251 ../gui/src/views/AdjunctPanel.qml:274
msgid "The total number of attachments is not more than six"
msgstr "Il numero totale degli allegati è superiore a sei"

#: ../gui/src/views/AdjunctPanel.qml:271
msgid "Please describe your idea in detail"
msgstr "Descrivi la tua idea in modo dettagliato"

#: ../gui/src/views/AdjunctPanel.qml:272
msgid "Please upload related attachments"
msgstr "Per cortesia carica eventuali allegati"

#: ../gui/src/views/Widgets/AdjunctTray.qml:163
msgid "Upload failed, please retry."
msgstr "Caricamento fallito, riprova per cortesia."

#: ../gui/src/views/Widgets/AppComboBox.qml:75
msgid "Please select feedback project firstly "
msgstr "Per cortesia seleziona prima il progetto"

#: ../gui/src/views/MainWindow.qml:250
msgid "Deepin User Feedback"
msgstr "Deepin User Feedback"

#: ../gui/src/views/MainWindow.qml:338
msgid "I have a problem"
msgstr "Ho un problema"

#: ../gui/src/views/MainWindow.qml:349
msgid "I have a good idea"
msgstr "Ho una buona idea"

#: ../gui/src/views/MainWindow.qml:367
#, qt-format
msgid "The draft of %1 has been saved."
msgstr "La bozza di %1 è stata salvata."

#: ../gui/src/views/MainWindow.qml:383
msgid "Please input the problem title"
msgstr "Scrivi il titolo del problema"

#: ../gui/src/views/MainWindow.qml:384
msgid "Please describe your idea simply"
msgstr "Descrivi la tua idea in modo semplice"

#: ../gui/src/views/MainWindow.qml:388
msgid "Title words have reached limit."
msgstr "Hai raggiunto la lunghezza massima."

#: ../gui/src/views/MainWindow.qml:419
msgid "Please fill in email to get the feedback progress."
msgstr "Compila il campo email per rimanere aggiornato."

#: ../gui/src/views/MainWindow.qml:422
msgid "Email is invalid."
msgstr "Email non valida."

#: ../gui/src/views/MainWindow.qml:476
msgid ""
"I wish to join in User Feedback Help Plan to quickly improve the system "
"without any personal information collected."
msgstr "Mi piacerebbe partecipare al User Feedback Help Plan per migliorare Deepin senza che vengano raccolte info personali."

